{
  "rewrites": [
    {
      "source": "/notice/:path*",
      "destination": "http://busopen.jeju.go.kr/OpenAPI/service/bis/Notice"
    },
    {
      "source": "/station/:path*",
      "destination": "http://busopen.jeju.go.kr/OpenAPI/service/bis/Station"
    },
    {
      "source": "/arrivalInfo",
      "has": [
        {
          "type": "query",
          "key": "stationId",
          "value": "stationId"
        }
      ],
      "destination": "https://bus.jeju.go.kr/api/searchArrivalInfoList.do"
    }
  ]
}
