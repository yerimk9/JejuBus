{
  "rewrites": [
    {
      "source": "/notice",
      "destination": "http://busopen.jeju.go.kr/OpenAPI/service/bis/Notice"
    },
    {
      "source": "/station",
      "destination": "http://busopen.jeju.go.kr/OpenAPI/service/bis/Station"
    },
    {
      "source": "/routeList*",
      "destination": "http://bus.jeju.go.kr/api/searchBusRouteList.do"
    },
    {
      "source": "/busLocation",
      "destination": "http://bus.jeju.go.kr/api/searchBusAllocList.do"
    },
    {
      "source": "/arrivalInfo",
      "has": [
        {
          "type": "query",
          "key": "station_id",
          "value": "stationId"
        }
      ],
      "destination": "https://bus.jeju.go.kr/api/searchArrivalInfoList.do"
    },
    {
      "source": "/busRouteStation/:path*",
      "has": [
        {
          "type": "query",
          "key": "routeId",
          "value": "routeId"
        }
      ],
      "destination": "http://bus.jeju.go.kr/api/searchBusRouteStationList.do?/:path*"
    },
    {
      "source": "/map",
      "has": [
        {
          "type": "query",
          "key": "keyword",
          "value": "busStop"
        }
      ],
      "destination": "https://maps.googleapis.com/maps/api/place/nearbysearch/json"
    }
  ]
}
