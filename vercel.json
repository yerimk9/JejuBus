{
  "rewrites": [
    {
      "source": "/notice/:path*",
      "destination": "http://busopen.jeju.go.kr/OpenAPI/service/bis/Notice"
    },
    {
      "source": "/station/:path*",
      "destination": "http://busopen.jeju.go.kr/OpenAPI/service/bis/Station"
    },
    {
      "source": "/arrivalInfo/:path*",
      "has": [
        {
          "type": "query",
          "key": "stationId",
          "value": "stationId"
        }
      ],
      "destination": "https://bus.jeju.go.kr/api/searchArrivalInfoList.do/:path*"
    },
    {
      "source": "/busRouteStation/:path*",
      "has": [
        {
          "type": "query",
          "key": "routeId",
          "value": "routeId"
        }
      ],
      "destination": "http://bus.jeju.go.kr/api/searchBusRouteStationList.do?/:path*"
    },
    {
      "source": "/routeList/:path*",
      "destination": "http://bus.jeju.go.kr/api/searchBusRouteList.do"
    },
    {
      "source": "/busLocation/:path*",
      "destination": "http://bus.jeju.go.kr/api/searchBusAllocList.do"
    }
  ]
}
