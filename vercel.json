{
  "rewrites": [
    {
      "source": "/notice/:path*",
      "destination": "http://busopen.jeju.go.kr/OpenAPI/service/bis/Notice"
    },
    {
      "source": "/station/:path*",
      "destination": "http://busopen.jeju.go.kr/OpenAPI/service/bis/Station"
    },
    {
      "source": "/arrivalInfo/:path*",
      "has": [
        {
          "type": "query",
          "key": "station_id",
          "value": "station_id"
        }
      ],
      "destination": "https://bus.jeju.go.kr/api/searchArrivalInfoList.do/:path*"
    }
  ]
}
